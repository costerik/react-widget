{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
    {
      "extensions": [
        ".js",
      ".jsx"
      ]
    }
    ],
    "prettier/prettier": [
      "error",
    {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 100
    }
    ]
  },
  "plugins": [
    "prettier"
  ],
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  },
}
